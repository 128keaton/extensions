{
  "name": "extensions-srcs",
  "version": "0.0.0-NOT-USED",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "build:site": "ng build demos --prod --baseHref=./",
    "deploy": "angular-cli-ghpages --no-silent --dir=dist/demos",
    "release": "npm run build:site && npm run deploy",
    "build:lib": "ng build extensions --prod",
    "copy:scss": "gulp --gulpfile gulpfile.js",
    "publish": "npm run build:lib && npm run copy:scss && cd dist/extensions && npm publish --access public"
  },
  "private": true,
  "dependencies": {
