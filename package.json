    "@angular/platform-browser-dynamic": "~9.0.6",
    "@angular/router": "~9.0.6",
    "@ng-select/ng-select": "^4.0.0",
    "ngx-color": "^5.0.3",
    "ngx-progressbar": "^6.0.2",
